{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/react",
        "eslint-config-react-redux",
        "plugin:react-redux/recommended"
    ],
    "rules": {
        "react/prop-types": 0,
        "react-redux/connect-prefer-named-arguments": 2,
        "import/named": 2
    },
    "plugins": [
        "react",
        "import",
        "jsx-a11y",
        "react-redux"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}